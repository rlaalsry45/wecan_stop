#wrap {
    background: url/usr/image/common/bg_gnb.gif') repeat-x 0 0;
    position: relative;
}

#header {
    width: 970px;
    height: 153px;
    margin: 0 auto;
    position: relative;
    z-index: 1;
}

#sub .l-cont, #normal .l-cont {
    width: 725px;
}

/* gnb */
#gnb {
    width: 970px;
    height: 44px;
    background: url/usr/image/common/bg_gnb2.gif') no-repeat 0 top;
    position: absolute;
    top: 71px;
    left: 0;
}

#gnb li {
    display: inline;
    float: left;
    height: 30px;
    padding-top: 14px;
    margin-left: 161px;
    text-align: center;
}

#gnb li#gnb1 {
    margin-left: 92px;
}

#gnb li ul {
    overflow: hidden;
    width: 970px;
    height: 37px;
    background: url/usr/image/common/bg_gnb01.png') no-repeat 0 top;
    position: absolute;
    top: 45px;
    left: 0;
    visibility: hidden;
}

#gnb li#gnb1 ul li.first {
    margin-left: 98px;
}

#gnb li#gnb2 ul li.first {
    margin-left: 207px;
}

#gnb li#gnb3 ul li.first {
    margin-left: 477px;
}

#gnb li#gnb4 ul li.first {
    margin-left: 72px;
}

/* 2013-07-29 */
.gnb_en {
    width: 970px;
    height: 44px;
    background: url('/usr/image/common/bg_gnb2.gif') no-repeat 0 top;
    position: absolute;
    top: 71px;
    left: 0;
}

.gnb_en li {
    display: inline;
    float: left;
    height: 30px;
    padding-top: 14px;
    margin-left: 125px !important;
    text-align: center;
}

.gnb_en li#gnb1 {
    margin-left: 92px !important;
}

.gnb_en li#gnb1 ul li.first {
    margin-left: 98px !important;
}

.gnb_en li#gnb2 ul li.first {
    margin-left: 207px !important;
}

.gnb_en li#gnb3 ul li.first {
    margin-left: 330px !important;
}

.gnb_en li#gnb4 ul li.first {
    margin-left: 72px !important;
}

.gnb_en li ul li {
    display: inline;
    float: left;
    width: auto;
    height: 27px;
    padding-top: 10px;
    margin-left: 40px !important;
}

.gnb_en li#gnb4 ul li {
    margin: 0 !important;
}

.gnb_en li ul li a {
    display: inline-block;
    height: 21px;
    font-size: 0;
    line-height: 0;
    vertical-align: top;
    overflow: hidden;
}

.gnb_en li ul li a:hover img, #gnb li ul li a:active img, #gnb li ul li a:focus img, #gnb li ul li a.hover img {
    margin-top: -21px !important;
}

/* 2013-07-29 */

/* �꾩껜硫붾돱 */
#fullmenu {
    display: none;
    width: 968px;
    padding-bottom: 19px;
    border: 1px solid #d1d1d1;
    background: #fcfcfc;
    position: absolute;
    top: 115px;
    left: 0;
    z-index: 1;
}

#fullmenu ul li {
    float: left;
    width: 24.9%;
    padding: 0;
    margin: 0;
    background: none;
    font-family: NANUM;
    font-size: 12px;
    line-height: 18px;
}

#fullmenu ul li ul {
    margin-left: 90px;
}

/* �쒕툕硫붾돱 */
#lnb {
    width: 220px;
    position: absolute;
    top: 0;
    right: 0;
}

#lnb h2 {
    width: 205px;
    height: 28px;
    padding: 10px 0 0 15px;
    background: url/usr/image/common/bg_lnb.gif') no-repeat 0 0;
    color: #fff;
    font-family: NANUM;
    font-size: 16px;
    line-height: 18px;
}

#lnb ul {
    padding-bottom: 17px;
    border-right: 1px solid #dbdbdb;
    border-bottom: 1px solid #dbdbdb;
    border-left: 1px solid #dbdbdb;
}

#lnb ul li {
    padding-top: 13px;
    font-family: NANUM;
    font-size: 12px;
    line-height: 18px;
    font-weight: bold;
}

#lnb ul li span a {
    display: block;
    color: #000000;
    padding-left: 22px;
    background: url/usr/image/common/icon/bl_lnb.gif') no-repeat 7px 6px;
}

#lnb ul li span.on a, #lnb ul li span a:hover {
    color: #db3e4c;
}

#lnb ul li ul {
    margin-top: 11px;
    padding: 11px 0 11px 10px;
    border-left: 0;
    border-right: 0;
    border-top: 1px solid #dbdbdb;
    border-bottom: 1px solid #dbdbdb;
    background: #fafafa;
}

#lnb ul li ul li {
    margin-top: 3px;
    padding: 0 0 0 8px;
    background: url/usr/image/common/icon/bl_dot.gif') no-repeat 0 7px;
    font-family: NANUM;
    font-size: 12px;
    line-height: 18px;
    font-weight: normal;
}

#lnb ul li ul li.first {
    margin-top: 0;
}

#lnb ul li ul li a {
    color: #5d5d5d;
}

#lnb ul li ul li.on a, #lnb ul li ul li a:hover, #lnb ul li ul li ul li em.on a, #lnb ul li ul li ul li em a:hover {
    color: #db3e4c;
}

#lnb ul li ul li ul {
    margin-top: 0;
    border: 0;
    padding: 5px 0 7px 0;
}

#lnb ul li ul li ul li {
    background: url/usr/image/common/icon/bl_lnb3.gif') no-repeat 0 7px;
}

#lnb ul li ul li.on ul li a {
    color: #5d5d5d;
    font-family: NANUM;
    font-size: 12px;
    line-height: 18px;
    font-weight: normal;
}

#lnb span.banner {
    display: block;
    margin-top: 15px;
}

/* container */
#container {
    overflow: hidden;
    width: 970px;
    margin: 0 auto;
    padding-top: 47px;
    position: relative;
}

#container .contents .l-cont {
    float: left;
}

#container .contents .r-cont {
    float: right;
}

/* location */
div.location {
    width: 725px;
}

/* �щ떒�덊럹�댁� */
div.homepage {
    overflow: hidden;
    float: left;
    width: 722px;
    height: 85px;
    border: 1px solid #e0e0e0;
}

div.homepage ul {
    float: left;
    width: 643px;
    padding-top: 8px;
}

div.homepage ul.banner {
    float: left;
    width: 643px;
    padding-top: 18px;
}

div.banner {
    margin-top: 14px;
}

div.banner a {
    display: block;
    margin-top: 11px;
}

div.banner a.first {
    margin-top: 0;
}

div.banner.mgt {
    margin-top: 10px;
}

/* 2013-09-24 */
div.customer {
    overflow: hidden;
    float: right;
    width: 230px;
    height: 85px;
    border: 1px solid #e0e0e0;
    background: #e4e4e4;
}

div.customer ul {
    overflow: hidden;
    float: right;
    width: 151px;
    padding-top: 16px;
}

/* �듯빀寃�� */
ul.search-photo {
    overflow: hidden;
    margin-top: 30px;
}

ul.search-photo li {
    float: left;
    width: 19.9%;
    text-align: center;
}

ul.search-photo li a img {
    border: 1px solid #c5c4c2;
}

ul.search-photo li strong {
    display: block;
    color: #000000;
    font-family: NANUM;
    font-size: 12px;
    line-height: 18px;
    text-align: center;
}

/* footer */
.f-menu {
    overflow: hidden;
    width: 970px;
    margin: 0 auto;
}

#footer .foot div {
    overflow: hidden;
    width: 970px;
    margin: 0 auto;
}