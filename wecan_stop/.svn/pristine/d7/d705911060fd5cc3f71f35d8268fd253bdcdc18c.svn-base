$(function(){
	
	var frm = $("form[name='frm']");
	$("#submit_a").click(function(){
		submit_a();
	});

	$("#submit_b").click(function(){
		submit_b();
	});

	$("#submit_c").click(function(){
		submit_c();
	});

	

});

function submit_a(){
	$("#usertype").val("a");
	frm.submit();
}

function submit_b(){
	$("#usertype").val("b");
	frm.submit();
}

function submit_c(){
	$("#usertype").val("c");
	$("#act").val("terms");
	frm.submit();
}