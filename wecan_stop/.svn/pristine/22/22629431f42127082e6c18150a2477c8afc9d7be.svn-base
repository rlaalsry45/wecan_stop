/* font */
@font-face {
    font-family: NANUM;
    src: url('/com/font/NanumGothic.eot');
    src: local('NanumGothic'),
    url('/com/font/NanumGothic.eot?#iefix') format('embedded-opentype'),
    url('/com/font/NanumGothic.woff') format('woff');
}

#popup {
    width: 832px;
    height: 700px;
    overflow-x: hidden;
    overflow-y: auto;
    position: relative;
}

#popup .pop-wrap {
    background: url('/usr/image/popup/bg_popup.gif') no-repeat 0 0;
}

#popup h1 {
    height: 127px;
    padding: 35px 0 0 200px;
    color: #486577;
    font-size: 35px;
    line-height: 40px;
    letter-spacing: -2px;
    font-weight: 400;
}

#popup h1.type {
    height: 112px;
    padding-top: 50px;
}

#popup h1 span {
    font-weight: 800;
}

#popup h1 strong {
    display: block;
    width: 120px;
    height: 81px;
    padding-top: 50px;
    background: #486577;
    color: #ffffff;
    font-size: 23px;
    line-height: 30px;
    font-weight: 800px;
    text-align: center;
    position: absolute;
    top: 0;
    left: 40px;
}

#popup h1 em {
    display: block;
    font-size: 27px;
}

.pop-cont {
    padding: 20px 40px;
}

ul.list li {
    padding-left: 16px;
    font-size: 14px;
    line-height: 20px;
    text-indent: -16px;
}

div.score {
    overflow: hidden;
    margin-top: 30px;
    color: #989898;
    font-size: 45px;
    line-height: 50px;
    text-align: center;
    font-weight: 800;
}

div.score div {
    float: left;
    width: 33%;
    margin: 0 auto;
}

div.score strong {
    display: block;
    color: #000000;
    font-size: 14px;
    line-height: 18px;
}

div.score em {
    color: #222222;
}

div.score span {
    display: inline-block;
    color: #d10000;
    font-size: 70px;
    line-height: 80px;
    font-weight: 800;
    vertical-align: -5px;
}

div.result {
    margin-top: 37px;
    padding-top: 40px;
    color: #0c5bb5;
    background: url('/usr/image/popup/bg_dotline.gif') repeat-x 0 top;
    font-size: 20px;
    line-height: 28px;
    text-align: center;
    font-weight: 800;
}

div.result span {
    color: #000000;
}

div.result p {
    color: #666666;
    font-size: 14px;
    line-height: 18px;
    font-weight: normal;
}

div.scoring {
    margin-top: 40px;
    padding: 20px;
    border: 1px solid #ededed;
    background: #f5f5f5;
}

div.scoring h2 {
    padding-left: 20px;
    background: url('/usr/image/common/icon/icon_standard.gif') no-repeat 0 0;
    color: #222222;
    font-size: 14px;
    line-height: 18px;
}

div.scoring ul {
    margin-top: 10px;
}

div.scoring ul.type, div.scoring ul.type2 {
    margin-top: 0;
}

div.scoring ul.type2 li {
    margin-top: 10px;
}

div.scoring ul.type2 li strong {
    float: none;
    display: block;
    width: auto;
}

div.scoring ul.type2 li p {
    margin-top: 0;
    color: #666666;
}

div.scoring ul li {
    overflow: hidden;
    padding-left: 10px;
    background: url('/usr/image/common/icon/bl_list.gif') no-repeat 0 8px;
}

div.scoring ul li strong {
    display: inline;
    float: left;
    width: 70px;
    color: #222222;
}

div.scoring ul li em {
    display: inline;
    float: left;
    width: 613px;
    color: #222222;
}

div.scoring h3 {
    margin-top: 30px;
    color: #222222;
}

div.scoring p {
    margin-top: 10px;
    color: #ae3051;
}

/* board */
table.data-table {
    width: 100%;
    margin-top: 50px;
    border-top: 2px solid #545454;
}

table.data-table th {
    border-bottom: 1px solid #e4e4e4;
    background: #f3f3f3;
}

table.data-table thead th {
    padding: 5px 0;
    border-left: 1px solid #e4e4e4;
}

table.data-table thead th.first {
    border-left: none;
}

table.data-table td {
    padding: 7px 0;
    border-bottom: 1px solid #e4e4e4;
    border-left: 1px solid #e4e4e4;
    text-align: center;
}

table.data-table td.first {
    border-left: 0;
}

table.data-table td.subject {
    padding-left: 15px;
    text-align: left;
}

/* 2014-08-11 �̰��� �߰� */
table.data-table tbody th.subject {
    padding-left: 15px;
    text-align: left;
    color: #000;
    font-weight: normal;
}

table.data-table td.subject input.radio {
    float: left;
    margin-top: 5px;
}

table.data-table td.subject select {
    float: left;
    margin-top: 2px;
    margin-right: 2px;
}

table.data-table td.subject label {
    float: left;
    margin-right: 20px;
    line-height: 25px;
    height: 20px;
    color: #565656;
}

/* btn */
div.btn-c {
    margin-top: 20px;
    text-align: center;
}

