#popup-sexual {
    width: 700px;
    position: relative;
    background: url('/usr/image/popup/bg_sexual_top.gif') no-repeat 0 0;
}

#popup-sexual h1 {
    font-size: 30px;
    color: #fff;
    line-height: 30px;
    width: 100%;
    text-align: center;
    padding: 50px 0 107px 0px;
}

.sexual-area {
    position: relative;
    padding: 40px 19px 65px 24px;
    font-size: 14px;
}

.sexual-area h2 {
    text-align: center;
    color: #303030;
    font-size: 25px;
    line-height: 25px;
    font-weight: bold;
    padding-bottom: 30px;
}

.sexual-area h3 {
    text-align: center;
    color: #303030;
    font-size: 16px;
    line-height: 23px;
    padding-top: 25px;
    font-weight: normal;
}

.sexual-area .sexual-box {
    width: 657px;
    background: url('/usr/image/popup/box_sexual_bg.gif') repeat-y 0 0;
}

.sexual-area .sexual-box .txt {
    text-align: center;
    font-size: 25px;
    line-height: 65px;
    font-weight: bold;
    padding-right: 50px;
}

.sexual-area .sexual-box .txt a {
    background: url('/usr/image/popup/icon_arrow.gif') no-repeat right 0;
    padding-bottom: 10px;
    padding-right: 50px;
    color: #303030;
}

a.btn_sexual01 {
    height: 36px;
    padding: 0 55px;
    border: 1px solid #4e8dc2;
    line-height: 36px;
    display: inline-block;
    background: #426c91;
    color: #fff;
    font-size: 17px;
    font-weight: bold;
}

a.btn_sexual01 hover {
    color: #fff;
}

.sexual-area .sexual-box .txt-info {
    padding: 20px 25px 0 25px;
}

.sexual-area .sexual-box .txt-info ul {
    width: 100%;
}

.sexual-area .sexual-box .txt-info ul li {
    line-height: 20px;
    font-size: 14px;
    width: 100%;
    padding-bottom: 20px;
}

.sexual-area .sexual-box .txt-info ul li.f_coler {
    color: #ff4200;
}

.sexual-area .sexual-box02 {
    width: 643px;
}

.sexual-area .sexual-box02 ul {
    float: left;
    width: 100%;
    padding-bottom: 50px;
}

.sexual-area .sexual-box02 ul li {
    float: left;
    margin-left: 17px;
}

.sexual-area .sexual-box02 ul li.first {
    margin-left: 0px;
}

p.txt_tip {
    width: 100%;
    font-size: 14px;
    color: #303030;
    margin-bottom: -45px;
    padding-top: 50px;
}

.sexual-area .sexual-box-end {
    padding-top: 200px;
    font-size: 25px;
    line-height: 25px;
    letter-spacing: -1px;
    font-weight: bold;
    color: #303030;
    background: url('/usr/image/popup/icon_balloon.gif') no-repeat center 0;
}